{
  "name": "schoen",
  "description": "Music theory and instrument models.",
  "version": "0.1.0",
  "author": "Oliver Steele <steele@osteele.com>",
  "license": "MIT",
  "keywords": [
    "music",
    "music-theory",
    "chord-diagrams"
  ],
  "repository": "github:osteele/schoen",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@types/lodash": "^4.14.116",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "husky": "^1.0.0-rc.13",
    "jest": "^23.4.2",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.1.2",
    "tslint": "^5.11.0",
    "typedoc": "^0.11.1",
    "typescript": "^3.0.1"
  },
  "scripts": {
    "prepublish": "yarn run build && tsc",
    "build": "rimraf dist && tsc",
    "doc": "typedoc --out docs/api src --mode modules --module commonjs",
    "lint": "tslint src/**/*.ts test/**/*.ts",
    "test": "jest --onlyChanged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint --fix && yarn test --bail",
      "pre-push": "yarn test --all"
    }
  }
}
